OLED_ENABLE = yes           # enable OLED display
OLED_DRIVER = SSD1306       # OLED driver
BOOTMAGIC_ENABLE =  no      # Virtual DIP switch configuration(+1000)
MOUSEKEY_ENABLE = no        # Mouse keys(+4700)
EXTRAKEY_ENABLE = yes       # Audio control and System control(+450)
CONSOLE_ENABLE = no         # Console Ensole for debug(+400)
COMMAND_ENABLE = no         # Commands for debug and configuration
MIDI_ENABLE = no            # MIDI controls
AUDIO_ENABLE = no           # Audio output on port C6
UNICODE_ENABLE = no         # Unicode
BLUETOOTH_ENABLE = no       # Enable Bluetooth with the Adafruit EZ-Key HID
SWAP_HANDS_ENABLE = no      # Enable one-hand typing
RGBLIGHT_ENABLE =  no       # Enable WS2812 RGB underlight.
RGB_MATRIX_ENABLE  = yes    # allow rgb matrix individual leds
VIA_ENABLE = yes            # enable via configurator
LTO_ENABLE = yes            # to reduce firmware size
WPM_ENABLE = no             # allows words per minute

# Do not enable SLEEP_LED_ENABLE. it uses the same timer as BACKLIGHT_ENABLE
SLEEP_LED_ENABLE = no    # Breathing sleep LED during USB suspend
